{
    "name": "quollvm",
    "displayName": "QuollVM Source",
    "description": "Syntax Highlighter for QuollVM Source Files",
    "version": "0.0.1",
    "publisher": "grendrake",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "QuollVM",
            "aliases": ["QuollVM Source File", "QuollVM"],
            "extensions": [".qc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "QuollVM",
            "scopeName": "source.quollvm",
            "path": "./syntaxes/quollvm.tmLanguage"
        }]
    }
}